{
    "$schema": "http://json.schemastore.org/template",
    "author": "Thiago Fialho",
    "classifications": ["Herald", "Worker"],
    "identity": "Herald.Template.Worker",
    "name": "Herald Template Worker",
    "shortName": "herald-worker",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "sourcename": "WorkerService",
    "preferNameDirectory": true,
    "placeholderFilename": "-.-",
    "symbols": {
        "no-database": {
            "description": "Put database sample into generated project.",
            "type": "parameter",
            "dataType": "bool",
            "defaultValue": "false"
        },
        "no-queue": {
            "description": "Put queue sample into generated project.",
            "type": "parameter",
            "dataType": "bool",
            "defaultValue": "false"
        },
        "no-externalapi": {
            "description": "Put external api sample into generated project.",
            "type": "parameter",
            "dataType": "bool",
            "defaultValue": "false"
        },
        "nodatabase": {
            "type": "computed",
            "value": "(no-database)"
        },
        "noqueue": {
            "type": "computed",
            "value": "(no-queue)"
        },
        "noexternalapi": {
            "type": "computed",
            "value": "(no-externalapi)"
        }
    },
    "primaryOutputs": [{
            "path": "WorkerService.Worker.sln"
        }
    ],
    "sources": [{
            "modifiers": [{
                    "rename": {
                        "dockerignore": ".dockerignore",
                        "gitignore": ".gitignore"
                    }
                }
            ]
        }
    ]
}
